{
   "type": "sqlite",
   "database": "db/database.db",
   "synchronize": false,
   "migrationsRun": true,
   "logging": true,
   "entities": [
      "dist/entity/**/*{.ts,.js}"
   ],
   "migrations": [
      "dist/migration/**/*{.ts,.js}"
   ],
   "subscribers": [
      "src/subscriber/**/*{.ts,.js}"
   ],
   "cli": {
      "entitiesDir": "src/entity",
      "migrationsDir": "src/migration",
      "subscribersDir": "src/subscriber"
   }
}